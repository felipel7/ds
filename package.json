{
  "name": "@gapin-ds/core",
  "description": "Gapin Design System",
  "main": "core/loader/index.cjs.js",
  "module": "core/loader/index.js",
  "types": "core/loader/index.d.ts",
  "files": ["core"],
  "exports": {
    ".": {
      "import": "./core/loader/index.js",
      "require": "./core/loader/index.cjs.js",
      "types": "./core/loader/index.d.ts"
    },
    "./loader": {
      "import": "./core/loader/esm/loader.js",
      "require": "./core/loader/cjs/loader.cjs.js",
      "types": "./core/loader/index.d.ts"
    },
    "./components": {
      "import": "./core/index.js",
      "types": "./core/loader/types/components.d.ts"
    },
    "./components/*": {
      "import": "./core/*.js",
      "types": "./core/loader/types/components/*.d.ts"
    }
  }
}
